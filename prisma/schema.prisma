// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Song {
    id Int @id @default(autoincrement())
    title String
    artist String
    rank Int
    key Int
    memo String?
}

model Tag {
    id Int @id @default(autoincrement())
    name String
}

model TagMap {
    id Int @id @default(autoincrement())
    songId Int
    tagId Int
}